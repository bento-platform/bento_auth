{
  "name": "bento-auth-js",
  "version": "4.2.0",
  "description": "Authentication library for web applications of Bento-Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "prepublishOnly": "npm run build",
    "lint": "npx eslint src",
    "buildpack": "rm -f ./packs/*.tgz && tsc && npm pack --pack-destination ./packs"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bento-platform/bento_auth_js.git"
  },
  "keywords": [
    "Bento",
    "Auth"
  ],
  "author": "Sanjeev Lakhwani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bento-platform/bento_auth_js/issues"
  },
  "homepage": "https://github.com/bento-platform/bento_auth_js#readme",
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "react": ">=18.0.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.21.1",
    "redux": "^4.2.0"
  },
  "dependencies": {
    "jose": "^5.2.3"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@types/react": "18.2.42",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.21.1",
    "redux": "^4.2.0",
    "typescript": "~5.4.3"
  }
}
